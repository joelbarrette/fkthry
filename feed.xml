---
layout: null
---
  
<?xml version="1.0" encoding="UTF-8"?>
<rss xmlns:itunes="http://www.itunes.com/dtds/podcast-1.0.dtd" version="2.0">
<channel>
  <title>{{ site.title | xml_escape }}</title>
  <link></link>
  <language></language>
  <copyright></copyright>
  <itunes:subtitle></itunes:subtitle>
  <itunes:author></itunes:author>
  <itunes:summary></itunes:summary>  
  <itunes:owner>
    <itunes:name></itunes:name>
    <itunes:email></itunes:email>
  </itunes:owner>
  <itunes:image href="" />
  <itunes:category text=""/>

    <item>
      <title>{{ post.title | xml_escape }}</title>
      <itunes:author></itunes:author>
      <itunes:subtitle></itunes:subtitle>
      <itunes:summary>{% if post.subtitle %}{{ post.subtitle | xml_escape }} - {% endif %}
        {{ post.content | strip_html | xml_escape | truncatewords: excerpt_length }}</itunes:summary>
      <itunes:image href="" />
      <enclosure url="" length="" type="audio/mpeg"/>
      <guid>{{ post.url | absolute_url }}</guid>
      <pubDate>{{ post.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
      <itunes:duration></itunes:duration>
    </item>

</channel>
</rss>
